  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real | kind = kind_phys
  intent = in
[rho]
  standard_name = air_density
  long_name = air density
  units = kg m-3
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real | kind = kind_phys
  intent = in
[qmin]
