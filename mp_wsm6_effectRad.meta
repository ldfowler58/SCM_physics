[ccpp-arg-table]
  name = mp_wsm6_effectRad_run
  type = scheme
[do_microp_re]
  standard_name = flag_for_cloud_effective_radii
  long_name = flag for computing effective radii for cloud water, cloud ice, snow
  units = flag
  dimensions = ()
  type = logical
  intent = in
[t]
  standard_name = air_temperature
  long_name = air temperature
  units =  K
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real | kind = kind_phys
  intent = in
[qc]
  standard_name = cloud_liquid_water_mixing_ratio
  long_name = cloud condensed water mixing ratio
  units = kg kg-1
  dimensions = (horizontal_loop_extent,vertical_layer_dimension)
  type = real | kind = kind_phys
  intent = in
[qi]
  standard_name = cloud_ice_mixing_ratio
